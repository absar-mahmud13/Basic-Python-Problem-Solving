# Dictionary mapping text to emojis
emoji_dict = {
    ":smile:": "ğŸ˜„",
    ":heart:": "â¤ï¸",
    ":thumbs_up:": "ğŸ‘",
    ":sun:": "â˜€ï¸",
    ":star:": "â­"
}
def convert_to_emoji(text):
    for key, value in emoji_dict.items():
        text = text.replace(key, value)
    return text

text = "Hello :smile: I love coding! :heart: Have a great day! :sun: â­"
converted_text = convert_to_emoji(text)
print(converted_text)
