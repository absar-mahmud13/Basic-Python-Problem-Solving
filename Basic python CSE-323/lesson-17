# Create a 2D list (matrix)
matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
print("Matrix:")
print(matrix)

# Print a specific element
print(matrix[0][2])

matrix[0][2] = 15
print("Matrix after modification:")
print(matrix)

print("Items in the matrix:")
for row in matrix:
    for item in row:
        print(item, end=' ')
    print()  
